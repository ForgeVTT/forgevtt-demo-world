{"_id":"nmxpPaFsutJo59ft","name":"Drawing","folder":"ulB77yo0Bu8BQ3qv","sort":700000,"flags":{},"pages":[{"name":"Drawing","type":"text","title":{"show":false,"level":1},"text":{"format":1,"content":"<p>@Drawing[Circle]@Trigger[click]@JournalEntry[rcBt5OviQQlaAOUP]{Circle}<br />@Drawing[Deletedraw]@Trigger[click]@ChatMessage[/PopMessage \"Delete Drawings\"]<br />@Drawing[Rectangle]@Trigger[click]@ChatMessage[/PopMessage \"Rectangle\"]<br />@Drawing[Polygon]@Trigger[click]@ChatMessage[/PopMessage \"Polygon\"]<br />@Drawing[Freehand]@Trigger[click]@ChatMessage[/PopMessage \"Freehand\"]<br /><br />@Drawing[HelpDraw]@Trigger[click]@ChatMessage[/PopMessage \"Drawing Help\"]<br />@Drawing[ClearDraw]@Trigger[click]@Macro[Q9mqgcb2mHugpoYz]{Drawings}</p>"},"_id":"b50LRre8xRh8QGDL","image":{},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"sort":0,"ownership":{"default":-1},"flags":{}}],"ownership":{"default":0,"FRJk2q4bH9IdLZDf":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}
{"_id":"oDFunJo0V3ggxuZd","name":"Hp Bar","folder":"I0fRwOJ7pqQH2Hsi","sort":450000,"flags":{},"pages":[{"name":"Hp Bar","type":"text","title":{"show":false,"level":1},"text":{"format":1,"content":"<p style=\"text-align:center\"><img src=\"worlds/forgevtt-demo-world/Assets/Tiles/low%20health.png\" width=\"213\" height=\"246\" /></p>\n<h3 style=\"text-align:center\">Some creatures may have a health bar, and usually, you will have your own.</h3>\n<h3 style=\"text-align:center\">(depending on your Gamemaster)</h3>\n<h3 style=\"text-align:center\">You'll get a small understanding of health percentage this way.<br /><br /></h3>\n<h3 style=\"text-align:center\">You can edit your HP directly by token, with (+/-) by right-clicking your token, then editing the HP.</h3>\n<p style=\"text-align:center\"><img src=\"worlds/forgevtt-demo-world/Assets/Gifs/HP.gif\" width=\"217\" height=\"215\" /></p>"},"_id":"u96vKS78u1ZzJWAB","image":{},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"sort":0,"ownership":{"default":-1},"flags":{}}],"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}
